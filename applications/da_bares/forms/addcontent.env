           return;
          }
       $('#box_'+what+'_wait').show();
       $.post(BASEMETHOD + 'add'+what, {nome:valdin},
           function(data) {
               $('#box_'+what+'_wait').hide();
                data= JSON.parse(data);
                responder(data, data.ok['id_'+what], keep);         
           });
   }

   function editar_din(what){
