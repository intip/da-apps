    <script type="text/javascript" tal:content="structure python:util.py2js(ID_SITE=id_site,
                                                                            SCHEMA=schema,
                                                                            BASEMETHOD=portal._getUrlAppMethod(meta_type=instance.meta_type, schema=schema, id_site=id_site),
